URxvt*scrollBar: false
URxvt.perl-ext-common:  default,matcher,url-select
URxvt.matcher.button:   1
URxvt.urlLauncher:     chromium
URxvt.underlineURLs:   true
URxvt.keysym.M-u:      perl:url-select:select_next
! Цветовая схема
*.foreground:   #d0d0d0
*.background:   #000000
*.cursorColor:  #d0d0d0

! black
*.color0:       #000000
*.color8:       #808080

! red
*.color1:       #ff0000
*.color9:       #ff0000

! green
*.color2:       #33ff00
*.color10:      #33ff00

! yellow
*.color3:       #ff0099
*.color11:      #ff0099

! blue
*.color4:       #0066ff
*.color12:      #0066ff

! magenta
*.color5:       #cc00ff
*.color13:      #cc00ff

! cyan
*.color6:       #00ffff
*.color14:      #00ffff

! white
*.color7:       #d0d0d0
*.color15:      #ffffff
! Будет ли курсор мигать
URxvt.cursorBlink:      true
! Цвет курсора
!URxvt.cursorColor:     yellow
! Цвет текста, находящегося под курсором
URxvt.cursorColor2:     red
!-*- Копирование -*-
URxvt.perl-ext-common: default,selection-to-clipboard,pasta,matcher,keyboard-select
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher:   /usr/bin/chromium
URxvt.underlineURLs:  True
URxvt.matcher.button: 1
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V:     perl:pasta:paste
! Comment this if you don't want copy when text is selected
URxvt.clipboard.autocopy: true
! Шрифты
URxvt*font:                     xft:DejaVU Sans Mono:size=12
